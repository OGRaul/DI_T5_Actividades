	<queryString language="SQL">
		<![CDATA[SELECT "ADDRESS"."FIRSTNAME",
"ADDRESS"."LASTNAME",
"ADDRESS"."STREET",
"ADDRESS"."CITY",
"PRODUCT"."NAME",
"POSITIONS"."DOCUMENTID",
"POSITIONS"."QUANTITY",
"POSITIONS"."PRICE",
"ADDRESS"."ID"
FROM "ADDRESS",
"DOCUMENT",
"POSITIONS",
"PRODUCT"
WHERE  "DOCUMENT"."ID" = "POSITIONS"."DOCUMENTID" 
 AND "POSITIONS"."PRODUCTID" = "PRODUCT"."ID"
  AND "DOCUMENT"."ADDRESSID" = $P{IDCliente}]]>
	</queryString>
